# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Quick Commerce Medicine Delivery API

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=11520
REFRESH_TOKEN_EXPIRE_MINUTES=43200

# CORS and Hosts
BACKEND_CORS_ORIGINS=https://your-frontend-domain.com,http://localhost:3000
ALLOWED_HOSTS=your-render-app.onrender.com,localhost

# Database (PostgreSQL for production)
DATABASE_URL=postgresql://username:password@host:port/database
POSTGRES_SERVER=your-postgres-host
POSTGRES_USER=your-postgres-user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DB=your-database-name
POSTGRES_PORT=5432

# Redis
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# SMS Configuration
SMS_API_KEY=your-sms-api-key
SMS_API_SECRET=your-sms-api-secret

# Email Configuration
SMTP_TLS=true
SMTP_PORT=587
SMTP_HOST=your-smtp-host
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
EMAILS_FROM_EMAIL=noreply@yourapp.com
EMAILS_FROM_NAME=Quick Commerce Medicine

# Superuser
FIRST_SUPERUSER=admin@quickcommerce.com
FIRST_SUPERUSER_PASSWORD=your-secure-password

# Environment
TESTING=false
